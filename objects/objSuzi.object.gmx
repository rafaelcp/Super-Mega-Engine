            image_index = 0;    //After touching a wall, we want the eye to close slowly (half-open the first 10 frames)
        }
        if moveTimer == 110
        {
            image_index = 1;    //Eye half-open
        }
        else if moveTimer &gt;= 120
        {
            image_index = 2;
            moving = true;
        }
