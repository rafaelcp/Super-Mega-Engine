    {
        if dir == "hor"
            xspeed = startDir * 3;
        else
            yspeed = startDir * 3;
            
        generalCollision();
        
        if (xspeed == 0 &amp;&amp; dir == "hor") || (yspeed = 0 &amp;&amp; dir == "ver")
        {
            moving = false;
            moveTimer = 0;
            image_index = 1; //Eye half-open
            startDir = -startDir;   //Change the direction
        }
        
        x += xspeed;
        y += yspeed;
    }
}
else
{
    if dead == true
    {
        moveTimer = 0;
        startDir = beginStartDir;
        moving = false;
        image_index = 0;
    }
}
